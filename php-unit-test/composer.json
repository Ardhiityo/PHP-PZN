{
  "name": "pzn/unitest",
  "description": "belajar unit test",
  "type": "project",
  "autoload": {
    "psr-4": {
      "Programmerzamannow\\BelajarPhpUnitTest\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      " Programmerzamannow\\BelajarPhpUnitTest\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "arya",
      "email": "aryaadi229@gmail.com"
    }
  ],
  "require": {},
  "require-dev": {
    "phpunit/phpunit": "9.5.5"
  },
  "scripts": {
    "test": "phpunit --configuration phpunit.xml",
    "test:product": "phpunit --configuration phpunit.xml --testsuite product",
    "test:counter": "phpunit --configuration phpunit.xml --testsuite counter"
  }
}
